{
  "name": "ticket-evolution-seatmaps",
  "version": "0.0.1",
  "description": "interactive seatmaps",
  "repository": {
    "url": "git@github.com:ticketevolution/seatmaps-client.git",
    "type": "git"
  },
  "main": "build/tevomaps.js",
  "author": "BRAD LARSON",
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=8"
  },
  "scripts": {
    "prebuild": "node ./internals/scripts/buildDllManifest.js",
    "build": "cross-env BUILDING_DLL=true webpack --config internals/webpack/webpack.dll.babel.js --color",
    "build:prod": "webpack --mode=production --config internals/webpack/webpack.prod.babel.js --color",
    "pretest": "flow",
    "test": "cross-env NODE_ENV=test jest --coverage --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --no-cache --watchAll",
    "analyze": "webpack --config internals/webpack/webpack.prod.babel.js --profile --json > stats.json",
    "example": "cd example && webpack-cli --progress",
    "example:build": "cd example && webpack-cli --progress",
    "example:dev": "cd example && webpack-dev-server --config webpack.config.dev.babel.js --progress",
    "example:clean": "rm -rf example/dist",
    "example:analyze": "cd example && webpack --profile --json >> stats.json"
  },
  "dllPlugin": {
    "path": "node_modules/seatmaps-dlls",
    "exclude": [
      "cross-env"
    ],
    "include": [
      "core-js",
      "preact"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": ".*\\.test\\.js$"
  },
  "dependencies": {
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "react-inlinesvg": "^0.8.2",
    "react-toggled": "1.2.6",
    "react-transition-group": "2.3.1",
    "svg-pan-zoom": "3.5.3",
    "unfetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "add-asset-html-webpack-plugin": "^3.0.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.4",
    "babel-preset-preact": "^1.1.0",
    "chai": "4.1.2",
    "compression-webpack-plugin": "1.1.11",
    "cross-env": "5.1.5",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "0.7.0",
    "expose-loader": "^0.7.5",
    "file-loader": "1.1.11",
    "flow-bin": "0.72.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "4.2.0",
    "imports-loader": "0.8.0",
    "jest-cli": "23.0.0-alpha.0",
    "localStorage": "1.0.3",
    "mini-css-extract-plugin": "^0.5.0",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "preact-jsx-chai": "2.2.1",
    "prettier": "1.12.1",
    "shelljs": "0.8.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "^4.1.5"
  }
}
